{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "zkdatafactories"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://zkvaults.vault.azure.net/"
		},
		"ls_adlsg_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://zkadlsg.dfs.core.windows.net"
		},
		"ls_azuesql_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "zkservers.database.windows.net"
		},
		"ls_azuesql_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().db_name}"
		},
		"ls_azuesql_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "zkadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "zkdatafactories_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"ls_adlsg_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsg_properties_typeProperties_url')]"
					},
					"ls_azuesql_db_properties_typeProperties_server": {
						"value": "[parameters('ls_azuesql_db_properties_typeProperties_server')]"
					},
					"ls_azuesql_db_properties_typeProperties_database": {
						"value": "[parameters('ls_azuesql_db_properties_typeProperties_database')]"
					},
					"ls_azuesql_db_properties_typeProperties_userName": {
						"value": "[parameters('ls_azuesql_db_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}